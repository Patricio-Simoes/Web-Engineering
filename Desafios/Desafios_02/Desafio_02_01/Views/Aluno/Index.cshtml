@{
    ViewData["Title"] = "Registar Aluno";
}

<div>
    <h1>Aluno registado com sucesso!</h1>
    <table class="table table-striped table-hover">
        <thead>
            <tr>
                <th class="text-end">Número</th>
                <th>Nome</th>
            </tr>
        </thead>
        <tbody>
            <tr>
                <td class="text-end">@TempData["numero"]</td>
                <td>@TempData["nome"]</td>
            </tr>
        </tbody>
    </table>
</div>
